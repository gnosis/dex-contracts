{
  "extends": "solhint:default",
  "plugins": [
    "prettier"
  ],
  "rules": {
    "quotes": [
      "error",
      "double"
    ],
    "max-line-length": [
      "error",
      129
    ],
    "max-states-count": [
      "error",
      18
    ],
    "not-rely-on-time": "error",
    "prettier/prettier": "error"
  }
}